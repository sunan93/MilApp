<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Servletlogin</servlet-name>
        <servlet-class>CollegePack.Servletlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletSignUp</servlet-name>
        <servlet-class>CollegePack.ServletSignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servletlogout</servlet-name>
        <servlet-class>CollegePack.Servletlogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubmitQuesServlet</servlet-name>
        <servlet-class>CollegePack.SubmitQuesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubmitAnsServlet</servlet-name>
        <servlet-class>CollegePack.SubmitAnsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubmitBlogServlet</servlet-name>
        <servlet-class>CollegePack.SubmitBlogServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servletlogin</servlet-name>
        <url-pattern>/Servletlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletSignUp</servlet-name>
        <url-pattern>/ServletSignUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servletlogout</servlet-name>
        <url-pattern>/Servletlogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitQuesServlet</servlet-name>
        <url-pattern>/SubmitQuesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitAnsServlet</servlet-name>
        <url-pattern>/SubmitAnsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitBlogServlet</servlet-name>
        <url-pattern>/SubmitBlogServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
